    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxt/typescript-build": "latest",
    "@types/node": "^20",
    "nuxt": "^3.8.0",
    "typescript": "^5",
    "vue-tsc": "^3.1.0"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
