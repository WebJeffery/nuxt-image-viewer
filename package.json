{
  "name": "@nuxtjs/viewer",
  "version": "1.0.0",
  "description": "A Nuxt module for ViewerJS - A JavaScript image viewer",
  "main": "dist/module.js",
  "types": "dist/module.d.ts",
  "files": [
    "dist",
    "runtime",
    "types"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:module": "nuxt-module-build --stub --no-declaration",
